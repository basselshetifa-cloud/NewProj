{
  "name": "Discord",
  "author": "YashvirGaming",
  "version": "1.0",
  "url": "https://discord.com/api/v9/users/@me",
  "method": "GET",
  "cookie_format": "json",
  "cookie_location": "header",
  "blocks": [
    {
      "id": "1",
      "type": "REQUEST",
      "url": "https://discord.com/api/v9/users/@me",
      "method": "GET",
      "headers": {
        "Authorization": "<COOKIE:token>",
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64)",
        "X-Super-Properties": "eyJvcyI6IldpbmRvd3MifQ=="
      },
      "save_response": "discord_response"
    },
    {
      "id": "2",
      "type": "PARSE",
      "source": "discord_response",
      "parse_type": "json",
      "captures": {
        "username": "$.username",
        "email": "$.email",
        "user_id": "$..id",
        "verified": "$..verified"
      }
    },
    {
      "id": "3",
      "type": "KEY CHECK",
      "conditions": [
        {
          "left": "<username>",
          "condition": "EXISTS",
          "right": ""
        },
        {
          "left": "<email>",
          "condition": "CONTAINS",
          "right": "@"
        }
      ],
      "success": "HIT",
      "failure": "BAD"
    }
  ],
  "success_keywords": ["username", "email", "id"],
  "failure_keywords": ["401", "Unauthorized"],
  "needs_stealth": true,
  "use_proxy": true,
  "timeout": 15
}